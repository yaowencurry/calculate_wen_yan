.history {
  padding: 20rpx;
}
.null-data {
  text-align: center;
  padding: 20rpx;
  color: #999;
  font-size: 28rpx;
}
.restart {
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #bfbfbf;
  margin-bottom: 20rpx;
}
.restart .tips {
  margin-left: 15rpx;
}
.resart-btn {
  background-color: #e84343;
  color: #fff;
  font-size: 28rpx;
  width: 230rpx;
  padding: 10rpx 20rpx;
  text-align: center;
  border-radius: 15rpx;
}
.data {
  display: flex;
  align-items: center;
  color: #0078d7;
  font-weight: 700;
}
.data image {
  width: 60rpx;
  height: 60rpx;
}